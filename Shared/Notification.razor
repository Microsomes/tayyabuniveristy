<div class="alert alert-danger mt-4" role="alert">
    <span class="oi oi-info mr-2" aria-hidden="true"></span>
    <strong>@Title</strong>

    <span class="text-nowrap">
    </span>
</div>

@code {
    // Demonstrates how a parent component can supply parameters
    [Parameter]
    public string Title { get; set; }
}
